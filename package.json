{
  "name": "express-graphql-custom",
  "version": "0.7.1",
  "description": "Production ready GraphQL HTTP middleware.",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">= 6.x"
  },
  "main": "src/index.js",
